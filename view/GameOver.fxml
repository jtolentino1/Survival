<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GameOverController">
   <top>
      <Pane fx:id="mainPane" prefHeight="720.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="gameOverImage" fitHeight="152.0" fitWidth="520.0" layoutX="380.0" layoutY="79.0" pickOnBounds="true" preserveRatio="true" />
            <Text fx:id="scoreText" fill="WHITE" layoutX="380.0" layoutY="289.0" strokeType="OUTSIDE" strokeWidth="0.0" text="SCORE:">
               <font>
                  <Font name="Arial" size="64.0" />
               </font>
            </Text>
            <Button fx:id="returnTitleButton" layoutX="507.0" layoutY="360.0" mnemonicParsing="false" onAction="#returnButtonClicked" text="Return to Title Screen">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </top>
</BorderPane>
